@import "../shared/_reset"
@import "../shared/_fonts"
@import "../shared/_global"

.wrapper
	width 1150px
	position relative
	margin 0 auto
	min-height 600px
	-moz-box-sizing: border-box; 
	-webkit-box-sizing: border-box; 
	box-sizing: border-box;

.border
	position absolute
	top 0
	left 0
	width 100%
	height 100%
	-moz-box-sizing: border-box; 
	-webkit-box-sizing: border-box; 
	box-sizing: border-box;
	border 10px solid #fff
	z-index 10
	pointer-events none

.pages

	.header
		text-align center

	.logo-labs
		position relative
		margin 0 auto
		top 50px
		display block

	.bold_line, .thin_line
		display none
		width 50px
		height 4px
		position relative
		margin 0 auto
		margin-top 20px
		background-color main_color

	.thin_line
		height 1px
		margin-top 15px

	.title
		display none
		font-family font_cond_book
		color main_color
		text-transform uppercase
		margin-top 15px

	#container
		position absolute